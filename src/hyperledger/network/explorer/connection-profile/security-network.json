{ 
	"name": "test-network",
	"version": "1.0.0",
	"client": {
		"tlsEnable": true,
		"id": "exploreradmin",
		"password": "exploreradminpw"
	},
	"enableAuthentication": true,
	"organization": "SecurityOrgMSP",
	"connection": {
		"timeout": {
			"peer": {
				"endorser": "300"
			},
			"orderer": "300"
		}
	},
	"channels": {
		"mychannel": {
			"peers": {
				"peer0.security.example.com": {},
				"peer0.integrity.example.com": {}
			}
		}
	},
	"organizations": {
		"SecurityOrgMSP": {
			"mspid": "SecurityOrgMSP",
			"adminPrivateKey": {
				"path": "/tmp/crypto/peerOrganizations/security.example.com/users/Admin@security.example.com/msp/keystore/priv_sk"
			},
			"peers": ["peer0.security.example.com"],
			"signedCert": {
				"path": "/tmp/crypto/peerOrganizations/security.example.com/users/Admin@security.example.com/msp/signcerts/Admin@security.example.com-cert.pem"
			}
		},
		"IntegrityOrgMSP": {
			"mspid": "IntegrityOrgMSP",
			"adminPrivateKey": {
				"path": "/tmp/crypto/peerOrganizations/integrity.example.com/users/Admin@integrity.example.com/msp/keystore/priv_sk"
			},
			"peers": ["peer0.integrity.example.com"],
			"signedCert": {
				"path": "/tmp/crypto/peerOrganizations/integrity.example.com/users/Admin@integrity.example.com/msp/signcerts/Admin@integrity.example.com-cert.pem"
			}
		}
	},
	"peers": {
		"peer0.security.example.com": {
			"tlsCACerts": {
				"path": "/tmp/crypto/peerOrganizations/security.example.com/peers/peer0.security.example.com/tls/ca.crt"
			},
			"url": "grpcs://peer0.security.example.com:11051"
		},
		"peer0.integrity.example.com": {
			"tlsCACerts": {
				"path": "/tmp/crypto/peerOrganizations/integrity.example.com/peers/peer0.integrity.example.com/tls/ca.crt"
			},
			"url": "grpcs://peer0.integrity.example.com:12051"
		}
	}
}
